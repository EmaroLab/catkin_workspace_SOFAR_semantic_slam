<launch>
  <node name="orb_slam2_ros_mono" pkg="orb_slam2_ros"
      type="orb_slam2_ros_mono" args="
          $(find orb_slam2_ros)/orb_slam2/Vocabulary/ORBvoc.txt
          $(find orb_slam2_ros)/orb_slam2/config/mono.yaml"
      output="screen">
       <param name="publish_pointcloud" type="bool" value="true" />
       <param name="localize_only" type="bool" value="true" />
       <param name="pointcloud_frame_id" type="string" value="map" />
       <param name="camera_frame_id" type="string" value="camera_link" />
  </node>
 <node pkg="rosbag" type="play" name="rosbag_play_semPoint"
       args="--clock $(find orb_slam2_ros)/ros/Bagfile/semantical_loci.bag"/>   
  
 <node pkg="rosbag" type="play" name="rosbag_play_map"
       args="--clock $(find orb_slam2_ros)/orb_slam2_ros/ros/Bagfile/point_cloud.bag"/>

</launch>
